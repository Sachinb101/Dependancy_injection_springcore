<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd">
         
        <bean class="com.sb.beans.Address" id="addrId">
         <property name="houseno" value="32"/>
          <property name="city" value="Solapur"/>
         <property name="pincode" value="413305"/>
         </bean>
         
         <bean class="com.sb.beans.Employee" id="empId">
         <property name="emp_id" value="101"/>
         <property name="name" value="Sachin Birajdar"/>
         <property name="address" ref="addrId"/>
         </bean>  
              
  </beans>